# SWF Platform Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/swf_database
PGHOST=localhost
PGPORT=5432
PGDATABASE=swf_database
PGUSER=username
PGPASSWORD=password

# Blockchain Configuration
BSC_RPC_URL=https://bsc-dataseed.binance.org/
POLYGON_RPC_URL=https://polygon-rpc.com/
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# Smart Contract Addresses
SWF_TOKEN_ADDRESS=0x83E17aeB148d9b4b7Be0Be7C87dd73531a5a5738
SWF_MASTER_DAO=0x1680F203Aafcbc64adAbD54C15472F7283aA8d08
SWF_MASTER_DAO_ULTIMATE=0xf1aFbA4804dB581f5eC43f7AFC3e522728ddDcF0
SOVRAN_LAUNCHPAD=0x0C0bfd4E4170411bc9665A368FaFaB3048883C4C

# API Keys (Optional - Platform works without these)
BSCSCAN_API_KEY=your_bscscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Payment Processing (Optional)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Session Configuration
SESSION_SECRET=your_secure_session_secret_here

# Platform Configuration
NODE_ENV=development
PORT=5000
REPLIT_DOMAINS=your-domain.replit.app

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Risk Management Settings
MAX_POSITION_SIZE=100000
VOLATILITY_THRESHOLD=0.15
INSURANCE_POOL_FEE=0.001
CIRCUIT_BREAKER_THRESHOLD=0.10

# Security Settings
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100
JWT_SECRET=your_jwt_secret_here

# Feature Flags
ENABLE_RISK_MANAGEMENT=true
ENABLE_MICRO_INVESTMENT=true
ENABLE_AI_OPTIMIZATION=true
ENABLE_STAKING=true
ENABLE_GOVERNANCE=true